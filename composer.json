{
    "name": "jiny/emoney",
    "description": "E-money and Point system for JinyPHP",
    "keywords": [
        "jinyPHP",
        "framework",
        "auth",
        "emoney",
        "point"
    ],
    "homepage": "http://www.jinyphp.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com",
            "homepage": "http://www.jinyphp.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "jiny/auth": "^0.5.0"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\Emoney\\": "src/"
        },
        "files" : [
            "Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\Emoney\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\Emoney\\JinyEmoneyServiceProvider"
            ]
        }
    }
}
